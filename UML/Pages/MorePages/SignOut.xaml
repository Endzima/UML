<Page x:Class="UML.Pages.MorePages.SignOut"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UML.Pages.MorePages"
      mc:Ignorable="d" 
      Height="Auto" Width="Auto"
      Title="SignOut">

    <Grid Name="SignOutGrid" Opacity="0">
        <TextBlock x:Name="SignOutText" FontFamily="pack://application:,,,/src/Fonts/#Brutal Type" FontSize="25" FontWeight="SemiBold" Height="40" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White"/>
    </Grid>
</Page>
